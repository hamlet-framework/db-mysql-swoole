{
  "name": "hamlet-framework/db-mysql-swoole",
  "type": "framework",
  "description": "Hamlet Framework / DB / MySQL / Swoole",
  "keywords": ["framework", "orm", "mysql", "swoole"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Vasily Kartashov",
      "email": "info@kartashov.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7 || ^8",
    "ext-swoole": "@stable",
    "psr/log": "@stable",
    "hamlet-framework/db": "@stable",
    "hamlet-framework/http-swoole": "@stable"
  },
  "require-dev": {
    "swoole/ide-helper": "@stable",
    "monolog/monolog": "@stable",
    "phake/phake": "@stable",
    "phpunit/phpunit": "^7 || ^8 || ^9",
    "squizlabs/php_codesniffer": "@stable",
    "vimeo/psalm": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Hamlet\\Database\\MySQLSwoole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hamlet\\Database\\MySQLSwoole\\": "tests"
    }
  }
}
